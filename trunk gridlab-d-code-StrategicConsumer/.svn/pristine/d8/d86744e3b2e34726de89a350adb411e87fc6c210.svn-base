clock {
	timestamp '2000-01-01 0:00:00';
	timezone EST+5EDT;
}

module tape;
module climate;
module residential;
module powerflow;

#set iteration_limit=10

object climate {
}

object house:1..5 {
	parent triplex_meter:*;
	floor_area random.normal(1500,300);
}

object lights:6..10 {
	parent house:id-5;
}

object player:11..15 {
	parent lights:id-5;
	property demand;
	file lightingDemand.glt;
	loop 100;
}

object range:16..20 {
	parent house:id-15;
}

object player:21..25{
	parent range:id-5;
	property demand;
	file rangeDemand.glt;
	loop 100;
}

object clotheswasher:26..30 {
	parent house:id-25;
}

object player:31..35{
	parent clotheswasher:id-5;
	property enduse_queue;
	file clotheswasherDemand.glt;
	loop 100;
}

object waterheater:36..40 {
	parent house:id-35;
}

object player:41..45{
	parent waterheater:id-5;
	property water_demand;
	file hotwaterDemand.glt;
	loop 100;
}

object microwave:46..50 {
	parent house:id-45;
}

object plugload:51..55 {
	parent house:id-50;
}

object dishwasher:56..60 {
	parent house:id-55;
}

object player:61..65{
	parent microwave:id-15;
	property demand;
	file microwaveDemand.glt;
	loop 100;
}

object player:66..70{
	parent plugload:id-15;
	property demand;
	file plugloadDemand.glt;
	loop 100;
}


object player:71..75{
	parent dishwasher:id-15;
	property demand;
	file dishwasherDemand.glt;
	loop 100;
}

object occupantload:76..80{
	parent house:id-75;
	number_of_occupants 4;
}

object player:81..85{
	parent occupantload:id-5;
	property occupancy_fraction;
	file occupancyFraction.glt;
	loop 100;
}

object triplex_meter:86..90 {
	phases AS;
	nominal_voltage 120;
	voltage_1 120 V;
	voltage_N 0 V;
	voltage_2 120 V;
}

object recorder:91..95 {
	parent house:id-90;
	property internal_gain;
	file kgrecorder.txt;
	interval 0;
	limit 1000;
}