// Timestamp tests

//#include "debugger.glm"
#set dateformat=ISO

class test {
	timestamp a;
	intrinsic create (object parent) {
		return 1;
	};
	intrinsic init (object parent) {
		char buffer[256];
		gl_output("a = %s", gl_strftime(a, buffer, 255));
		return 1;
	};
}

object test {
	a '2002-03-24 17:32:28';
}