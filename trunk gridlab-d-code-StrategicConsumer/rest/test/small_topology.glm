// rest module example topology

clock{
     timezone PST+8PDT;
     timestamp '2000-01-01 0:00:00';
     stoptime '2000-01-01 0:00:00';
}

#set profiler=1
#set pauseatexit=1
//*********************************************
// modules
module tape;
module residential;
module climate;
module powerflow{
      solver_method NR;
      default_maximum_voltage_error 1e-6;
};
module rest{
};

object server{
      port 8080;
};
//*********************************************
object regulator {
     name R2-25-00-1_reg_1;
     from R2-25-00-1_node_324;
     to R2-25-00-1_meter_73;
     phases ABCN;
     configuration feeder_reg_cfg;
}

object regulator_configuration {
     name feeder_reg_cfg;
     Control OUTPUT_VOLTAGE;
     band_center 14975.0;
     band_width 240;
     connect_type WYE_WYE;
     time_delay 30;
     raise_taps 16;
     lower_taps 16;
     regulation 0.10;
     tap_pos_A 0;
     tap_pos_B 0;
     tap_pos_C 0;
}

object underground_line_conductor:1 {
     //name 1000 AAC 25kV (Conc);
